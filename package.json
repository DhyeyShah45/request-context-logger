{
  "name": "request-context-logger",
  "version": "0.1.4",
  "description": "A request-scoped logger with AsyncLocalStorage for Express and beyond.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && npm link",
    "dev": "tsup src/index.ts --watch --dts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "logger",
    "express",
    "asynclocalstorage",
    "context",
    "request"
  ],
  "author": "Dhyey Shah",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/moment": "^2.11.29",
    "@types/node": "^20.19.7",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.32.0",
    "moment": "^2.30.1",
    "tsup": "^7.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "kleur": "^4.1.5",
    "uuid": "^9.0.0"
  }
}
